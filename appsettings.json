{
    "spring": {
	"application": {
	    "name": "grabbymon"
	},
	"cloud": {
	    "config": {
		"uri": "http://configserver.cloudnativedotnet.com"
	    }
	}
    },
	"eureka": {
		"client": {
			"serviceUrl": "http://eureka.cloudnativedotnet.com/eureka/",
		    "shouldRegisterWithEureka": false
		},
		"instance": {
			"preferIpAddress": true,
			"virtualHostName": "blah",
			"port": 5000,
			"instanceId": "localhost:grabbymon:5000"
		}
	},	
    "Logging": {
	"IncludeScopes": false,
	"LogLevel": {
	    "Default": "Debug",
	    "System": "Information",
	    "Microsoft": "Information"
	}
    }
}
